# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'MLSeriesDemonstratorIOS' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for MLSeriesDemonstratorIOS
  # Image Labeling
  pod 'GoogleMLKit/ImageLabeling', '3.2.0'
  pod 'GoogleMLKit/ImageLabelingCustom', '3.2.0'
  
  # Face detection
  pod 'GoogleMLKit/FaceDetection', '3.2.0'
  
  # Object detection
  pod 'GoogleMLKit/ObjectDetection', '3.2.0'
  # If you want to use the base implementation:
  pod 'GoogleMLKit/PoseDetection', '3.2.0'

  # If you want to use the accurate implementation:
  pod 'GoogleMLKit/PoseDetectionAccurate', '3.2.0'
  
  # To recognize Latin script
  pod 'GoogleMLKit/TextRecognition', '3.2.0'
  # To recognize Devanagari script
  pod 'GoogleMLKit/TextRecognitionDevanagari', '3.2.0'
  
  # Tensor flow lite custom models
  pod 'TensorFlowLiteSwift'

  target 'MLSeriesDemonstratorIOSTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'MLSeriesDemonstratorIOSUITests' do
    # Pods for testing
  end

end
